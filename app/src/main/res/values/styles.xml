<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="colorControlNormal">#0094d4</item>
        <item name="colorControlActivated">#3366CC</item>
        <item name="colorControlHighlight">#3366CC</item>

    </style>

    <style name="AppTheme.BrandedLaunch" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="android:windowBackground">@drawable/logo</item>
    </style>
    <style name="TextAppearence.App.TextInputLayout" parent="@android:style/TextAppearance">
        <item name="android:textColor">@color/colorPrimary</item>
    </style>

</resources>
